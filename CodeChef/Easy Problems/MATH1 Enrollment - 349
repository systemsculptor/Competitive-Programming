int main() {
	
	
	int t;
	std::cin >> t;
	for(int i = 0; i < t; i++) 
	{
	    int x;
	    int y;
	    std::cin >> x >> y;
	    if(x > y ) 
	    {
	        std::cout << 0 << "\n";
	    }
	    else
	    {
	       int totalAmount = y - x; 
	       std::cout << totalAmount << "\n"; 
	    }
	   
	    
	}
	return 0;
}
