#include <iostream>


int main() {
    int t;
    std::cin >> t;
    for(int i = 0; i < t; i++)
    {
        int person;
        std::cin >> person;
        if(7 > person)
        {
            std::cout << "YES" << "\n";
        }
        else {
            std::cout << "NO" << '\n';
        }
    }
	return 0;
}
