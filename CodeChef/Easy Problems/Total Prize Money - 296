#include <iostream>


int main() {
	int t;
	std::cin >> t;
	for(int i = 0; i < t; i++){
	    int top;
	    int medium;
	    std::cin >> top >> medium;
	    int prizeFirst = 10 * top;
	    int prizeSecond = 90 * medium;
	    int totalAmount = prizeFirst + prizeSecond;
	    std::cout << totalAmount << "\n";
	  
	}
	return 0;
}
