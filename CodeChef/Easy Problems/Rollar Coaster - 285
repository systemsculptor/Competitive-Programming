#include <iostream>


int main() {
	int t;
	std::cin >> t;
	for(int i =0; i < t; i++)
	{
	    int height;
	    int trainHeight;
	    std::cin >> height >> trainHeight;
	    if (height >= trainHeight)
	    {
	        std::cout << "YES" << "\n";
	    }
	    else {
	        std::cout << "NO" << "\n";
	    }
	}
	return 0;
}
